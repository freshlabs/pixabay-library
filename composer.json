{
    "name": "webeweb/pixabay-library",
    "type": "library",
    "description": "Integrate Pixabay API with your projects",
    "keywords": [
        "API",
        "library",
        "Pixabay",
        "photo",
        "video",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/pixabay-library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "webeweb/core-library": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^7.0"
    },
    "support": {
        "issues": "https://github.com/webeweb/pixabay-library/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Pixabay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Pixabay\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
